<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.8.1//EN" 
	"http://gwtproject.org/doctype/2.8.1/gwt-module.dtd">
	
<module>
	<inherits name='com.google.gwt.user.User' />
	<inherits name="com.google.gwt.i18n.I18N" />

	<stylesheet src="codemirror/lib/codemirror.css" />
	<stylesheet src="codemirror/theme/eclipse.css" />
	<stylesheet src="codemirror/codemirrorCustom.css" />
	<stylesheet src="codemirror/addon/hint/show-hint.css" />

	<set-configuration-property name="CssResource.enableGss"
		value="true" />
	<set-configuration-property name="CssResource.conversionMode"
		value="strict" />

	<source path="client" />
	<public path="public" />

</module>
